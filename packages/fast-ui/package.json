{
  "name": "fast-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href /",
    "watch": "ng build --base-href / --watch --configuration development",
    "test": "ng test",
    "prepwa": "npm run build",
    "pwa": "lite-server -c bs-config.js",
    "page": "ng build --output-path ../../docs --base-href /",
    "postpage": "shx cp ../../docs/index.html ../../docs/404.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@angular/service-worker": "13.2.4",
    "@types/superagent": "^4.1.15",
    "hypothesis-data": "0.0.3",
    "ngx-markdown": "^13.1.0",
    "primeflex": "^3.1.3",
    "primeicons": "^5.0.0",
    "primeng": "^13.4.1",
    "rxjs": "~7.5.0",
    "superagent": "^7.1.1",
    "text-fragments-polyfill": "^5.2.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.5",
    "@angular/cli": "~13.2.5",
    "@angular/compiler-cli": "~13.2.0",
    "@types/chrome": "^0.0.179",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lite-server": "^2.6.1",
    "shx": "^0.3.4",
    "typescript": "~4.5.2"
  }
}
